# General EXAMPLE envs you will have to move this to hubspot_contact_form_sync/.envs/.production
# ------------------------------------------------------------------------------
# DJANGO_READ_DOT_ENV_FILE=True
DJANGO_SETTINGS_MODULE=config.settings.production
DJANGO_SECRET_KEY=your-secret-key
DJANGO_ADMIN_URL=your-admin-url
DJANGO_ALLOWED_HOSTS=your-allowed-hosts
DJANGO_CSRF_TRUSTED_ORIGINS=your-trusted-origins



# Security
# ------------------------------------------------------------------------------
# TIP: better off using DNS, however, redirect is OK too
DJANGO_SECURE_SSL_REDIRECT=False

# Email
# ------------------------------------------------------------------------------
DJANGO_SERVER_EMAIL=

MAILGUN_API_KEY=
MAILGUN_DOMAIN=


# AWS
# ------------------------------------------------------------------------------
DJANGO_AWS_ACCESS_KEY_ID=your-access-key-id
DJANGO_AWS_SECRET_ACCESS_KEY=your-secret-access-key
DJANGO_AWS_STORAGE_BUCKET_NAME=

# django-allauth
# ------------------------------------------------------------------------------
DJANGO_ACCOUNT_ALLOW_REGISTRATION=True

# Gunicorn
# ------------------------------------------------------------------------------
WEB_CONCURRENCY=4


# Redis
# ------------------------------------------------------------------------------
REDIS_URL=redis://redis:6379/0


# Hubspot
HUBSPOT_ACCESS_TOKEN=your-access-token
HUBSPOT_CLIENT_SECRET=your-client-secret
